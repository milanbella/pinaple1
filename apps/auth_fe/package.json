    "pinaple_www": "*"
  },
  "devDependencies": {
    "@types/react": "17.0.24",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "sass": "^1.42.1",
    "typescript": "4.4.3",
    "prettier": "^2.4.1"
  }
